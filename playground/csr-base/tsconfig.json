{
  "include": [
    "script",
    "src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2022",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "noUnusedLocals": false,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "jsx": "react"
  }
}